programa
{
	
	funcao inicio()
	{
		inteiro numero, contador=1, soma=0

		escreva("Informe um número de 1 a 10: ")
		leia(numero)

		enquanto(contador <= 10) {
			escreva(numero*contador, " : ", numero, " = ", contador, "\n")
			contador = contador+1
	}
}
}
