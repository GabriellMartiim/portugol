programa
{
	
	funcao inicio()
	{	real	valpo,percentualrendimento,valorAposRendimento
	
		escreva("Digite o valor do deposito na poupança :")
		leia(valpo)
		percentualrendimento = 0.05
		valorAposRendimento = valpo + (valpo * percentualrendimento)
		escreva("O rendimento na poupança foi de R$", valorAposRendimento)
	}
}
