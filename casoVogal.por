programa
{
	
	funcao inicio()
	{	caracter diasem
		escreva("escolha uma vogal:")
		leia(diasem)
		escolha(diasem){
			caso 'A': 
			escreva("vogal letra E")
			pare
			caso 'E': 
			escreva("vogal letra I")
			pare
			caso 'I':
			escreva("vogal letra O")
			pare
			caso 'U':
			escreva("vogal letra U não tem subsequente")
			pare
			caso contrario:
			escreva("opção invalida")
			
		}
		
	}
}
