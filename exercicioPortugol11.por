programa
{
	
	funcao inicio()
	{	inteiro num,divi,resto
	
		escreva("Insira um  numero inteiro : ")
		leia(num)
		divi= num /3
		resto = num % 3
		escreva("\no quonciente é: ", divi)
		escreva("\no resto é: ", resto)
		
	}
}
