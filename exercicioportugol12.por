programa
{
	
	funcao inicio()
	{	real	kg,consumodiario,duracao
		
		
		escreva("Qual a quantidade de alimento em quilos : ")
		leia(kg)
		consumodiario = 50
		duracao = (kg*1000)/consumodiario
		escreva("o alimento irá durar ",duracao, " dias.")
		
		
	}
}

