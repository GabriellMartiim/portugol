programa
{
	
	funcao inicio()
	{
		
		inteiro idd
		escreva("informe sua idade:")
		leia(idd)
		se(idd <18)
		escreva("voce é menor de idade")
		senao  {
			se(idd >= 18 e idd <= 64) {
			escreva("maior de idade")}
	}
	}
	}

