programa
{
	
	funcao inicio()
	{	inteiro valor1,valor2
	
		escreva("informe um valor inteiro: ")
		leia(valor1)
		escreva("informe um segundo valor inteiro: ")
		leia(valor2)
		escreva("\nA soma dos dois valores é de ", valor1+valor2)
		escreva("\nA subtração dos dois valores é de ",valor1-valor2)
	}	
}

