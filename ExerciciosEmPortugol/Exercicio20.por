programa
{
	
	funcao inicio()
	{
		inteiro idade
		escreva("informe sua idade: ")
		leia(idade)
		se(idade < 18){
			escreva("você é menor de idade")
		}senao se(idade >= 18 e idade <=64){
		escreva("maior de idade")
		}senao{
			escreva("voce é idoso")}
			
		}
			
	}
