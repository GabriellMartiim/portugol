programa
{
	
	funcao inicio()
	{	
		inteiro numero
		escreva("informe um numero de 1 até 7: ")
		leia(numero)
		escolha(numero){
			caso 1:
			escreva("segunda-feira")
			pare
			caso 2:
			escreva("terça-feira")
			pare
			caso 3:
			escreva("quarta-feira")
			pare
			caso 4:
			escreva("quinta-feira")
			pare
			caso 5:
			escreva("sexta-feira")
			pare
			caso 6:
			escreva("sabado")
			pare
			caso 7:
			escreva("domingo")
			pare
			caso contrario:
			escreva("ERROR404! TENTE NOVAMENTE")
			
		}
	}
}


