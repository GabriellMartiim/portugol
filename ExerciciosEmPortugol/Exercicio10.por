programa
{
	
	funcao inicio()
	{	
		inteiro valor1
		
		escreva("informe um valor :")
		leia(valor1)
		escreva("\n",valor1,"+",valor1,"+",valor1,"+",valor1,"+",valor1)
		escreva("\nSomando 5 vezes o valor fornecido fica em: ",valor1*5)
		escreva("\nA media é de ", (valor1*5)/5)
	}
}


