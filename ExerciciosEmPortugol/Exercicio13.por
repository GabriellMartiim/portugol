programa
{
	
	funcao inicio()
	{	
		inteiro valor,resto,quociente
		
		escreva("informe um valor de numero inteiro: ")
		leia(valor)
		quociente = valor/3
		resto = valor%3
		escreva("o quocie é ", quociente)
		escreva(" o resto da divisão é de ", resto)
	}
}
