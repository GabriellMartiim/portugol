programa
{
	
	funcao inicio()
	{	
		real QuantidadeCombu,Distancia
		
		escreva("Informe a quantidade de combustível abastecido: ")
		leia(QuantidadeCombu)
		escreva("Informe a distancia que o carro faz por litro: ")
		leia(Distancia)
		escreva("A distancia maxima que o automovel irá percorrer é de: ", QuantidadeCombu*Distancia,"km")
	}
}

