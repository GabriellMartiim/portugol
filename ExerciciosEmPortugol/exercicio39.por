programa
{
	
	funcao inicio()
	{
		real VariavelA,VariavelB, trocaA,trocaB
		
		escreva("informe um valor para a variável A: ")
		leia(VariavelA)
		escreva("informe um valor para a variável B: ")
		leia(VariavelB)
		trocaA = VariavelB
		trocaB = VariavelA
		escreva("\nos valores das variáveis antes \nA:",VariavelA,"\nB:",VariavelB,"\ndepois da troca \nA:",trocaA,"\nB:",trocaB)
		
		
	}
}

