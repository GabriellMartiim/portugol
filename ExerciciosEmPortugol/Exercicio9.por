programa
{
	
	funcao inicio()
	{
	
		inteiro filho1,filho2,mediafilho, sal1,sal2,mediasal
	
		
		escreva("infrome so salario do habitante 1: ")
		leia(sal1)
		escreva("informe quantos filhos o habitante 1 tem: ")
		leia(filho1)
		escreva("infrome so salario do habitante 2: ")
		leia(sal2)
		escreva("informe quantos filhos o habitante 2 tem: ")
		leia(filho2)
		mediasal = (sal1+sal2)/2 
		mediafilho = (filho1+filho2)/2
		escreva("a media de salario é de: ",mediasal)
		escreva("a media de filhos é de: ", mediafilho)
	}
}
      
