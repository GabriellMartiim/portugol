programa
{
	
	funcao inicio()
	{	real	ganha,mes,total
	
		escreva("Informe valor que o funcionário ganha por hora : ")
		leia(ganha)
		escreva("Informe quantas horas o funcionario trabalha por mes :")
		leia(mes)
		total = ganha*mes
		escreva("O salário total é de R$",total)
		
	
	}
}
